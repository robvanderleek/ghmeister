{
  "name": "repomeister",
  "version": "0.0.1",
  "author": "",
  "description": "GitHub Actions and Workflows made easy",
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]"
        }
      ]
    ]
  },
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.9.1",
    "@types/signale": "^1.4.7",
    "semantic-release": "^22.0.8",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "signale": "^1.4.0"
  }
}
